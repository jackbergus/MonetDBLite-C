module(unix);
module(pathfinder);
var TSTSRCDIR := getenv("TSTSRCDIR");
var names := bat(void,str,4).seqbase(0@0);
names := names.append("archive.xml");
names := names.append("copy1.xml");
names := names.append("copy2.xml");
names := names.append("log.xml");
names := names.access(BAT_READ);

var locations := [+](TSTSRCDIR+"/", names);
var colnames  := names; #.project("AddressBook");
var pageFrees := names.project(9LL);

shred_doc(locations, names, colnames, pageFrees);
