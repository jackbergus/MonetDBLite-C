INCLUDES = ../include $(MONETDB_INCS)

# avoid "[PFty_is2ns]:ube: error: Assert has been violated at '/set/mars_patch/builds.intel-S2/nightly.Fri/intel-S2/lang/ube/hf/src/type.c 1583'"
# with Sun Studio 12 compiler and -xOn & n>1
subtyping_CFLAGS = $(SUN_NOOPT_CFLAGS)

lib_semantics = {
	NOINST
	DIR = libdir
	SOURCES = \
		functions.c \
		normalize.brg \
		heuristic.c \
		ns.c \
		options.c \
		subtyping.c \
		typecheck.brg \
		types.c \
		varscope.c \
		xquery_fo.c 
}

lib_ns = {
	NOINST
	DIR = libdir
	SOURCES = \
		ns.c
}

EXTRA_DIST_DIR = tests
