@echo off

if not "%1"=="" goto skip
call %0 -r
goto end

:skip

setlocal

rem default back end is MonetDB4
set V=4
if "%1" == "-4" set V=4& shift
if "%1" == "-5" set V=5& shift

set pkg=monetdb-xquery
set builddir=@XPATHFINDER_BUILD@
set srcdir=@XPATHFINDER_SOURCE@

set PATH=%builddir%\conf;%PATH%

REM execute Mapprove.py in the source directory
pushd %srcdir%

call Mapprove.py -%V% --package=%pkg% "--TSTTRGBASE=%builddir%" %1 %2 %3 %4 %5 %6 %7 %8 %9

popd
endlocal

:end
