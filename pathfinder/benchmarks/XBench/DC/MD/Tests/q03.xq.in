let $doc :=
for $d in doc("$TSTTRGDIR/load_docs.xml")//doc/text()
return doc($d)
for $a in distinct-values($doc/order[total > 11000.0]/customer_id)
let $b := $doc/order[customer_id=$a]
return
    <Output>
        <CustKey>{$a}</CustKey>
        <NumberOfOrders>{count($b)}</NumberOfOrders>
    </Output>
