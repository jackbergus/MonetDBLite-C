let $doc :=
for $d in doc("$TSTTRGDIR/load_docs.xml")//doc/text()
return doc($d)
for $a in distinct-values ($doc/article/prolog/dateline/date)
let $b := $doc/article/prolog/dateline[date=$a]
return
    <Output>
        <Date>{$a}</Date>
        <NumberOfArticles>{count($b)}</NumberOfArticles>
    </Output>
