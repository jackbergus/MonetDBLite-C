declare namespace xlink='http://www.w3.org/1999/xlink';

let $doc :=
	for $d in doc("$TSTTRGDIR/load_docs.xml")//doc/text()
	return doc($d)
for $refID in distinct-values($doc/*//link/@xlink:href)
let $refDocs := distinct-values($doc/*[.//link/@xlink:href=$refID]/@doc_id)
where count($refDocs) >= 4
return
  <docid>
    {
      string($refID)
    }
  </docid>
