let $doc :=
	for $d in doc("$TSTTRGDIR/load_docs.xml")//doc/text()
	return doc($d)
let $b :=
  (for $a in $doc/directory//doc_info[@update_time]
   where not(empty($doc/*[@doc_id=$a/@doc_id]/@author))
   order by zero-or-one($a/@update_time) descending
   return
     <docid>
     {
     data($a/@doc_id)
     }
     </docid>
  )
return $b[position() <= 100]
