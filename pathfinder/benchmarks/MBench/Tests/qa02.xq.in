declare namespace my='my-functions';
declare function my:one_level($e as element()*)
{
if(empty($e)) then ()
else
   <average avgaSixtyFour="{exactly-one(avg(for $a in $e return $a/@aSixtyFour))}" aLevel="{exactly-one($e[1]/@aLevel)+1}">
    {my:one_level($e/eNest)}
  </average>
};
my:one_level(doc("$TSTTRGDIR/sf0.1.xml")/eNest)
