#!/bin/sh

pdd="`monetdb-clients-config --pkgdatadir`"
jar="`ls $pdd/lib/jdbcclient-*.*.jar | head -n1`"
jar="`$CYGPATH_WP "$jar"`"

LANG="en_US.UTF-8" 
export LANG

cat << EOF > .monetdb
user=guest
password=anonymous
EOF

cat << EOMIL > $TST.mil
	print("\303\251\303\251n");
EOMIL

java -jar "$jar" -h $HOST -p $MAPIPORT -l mil -f $TST.mil
