module("alarm");

var t0 := time();
sleep(15);
var t1 := time();
print(((t1 - t0) + 500) / 1000);
alarm(15, "print(\"alarm went off\");" );
printf("#~BeginVariableOutput~#\n");
timers().print();
time().print();
usec().print();
epoch().print();
ctime().print();
printf("#~EndVariableOutput~#\n");
sleep(16); # wait for alarm

quit();
