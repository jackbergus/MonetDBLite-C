setoid(oid(20000000));
# Selfcontained test of delta operations
module("trans");
var b:=new(int,int);
var bi:=new(int,int);
var bd:=new(int,int);
insert(b,1,2);
insert(b,2,3);
bd:=b.delta();
print(bd);
var a:= b.alpha();
a.print();

#commit the changes and repeat the action
clear(bd);
commit(b);
b.alpha().print();

insert(b,4,5);
delete(b,2,3);
print(b);
bd:=b.delta();
print(bd);
a:= b.alpha();
a.print();
abort(b);
print(b);
quit();
