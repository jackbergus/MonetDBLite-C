module(malalgebra);

var id:=new(void,oid).seqbase(0@0).append(0@0).append(2@0);
var aa:=new(void,int).seqbase(0@0).append(0).append(2);

# these two joins should be equivalent
var j:=join(id.reverse(), aa).rename("T_join");
var lj:=leftjoin(id.reverse(), aa).rename("T_leftjoin");

# these two joins should be equivalent
var j2:=join(id.reverse(), aa).reverse().rename("T_join_rev");
var lj2:=leftjoin(id.reverse(), aa).reverse().rename("T_leftjoin_rev");

ls("join");
dir("join");
