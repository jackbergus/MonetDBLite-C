
VAR b := new(int,int);
VAR n := 10;
VAR i := 0;

WHILE (i < n) {
	i :+= 1;
	b.insert(i,i);
}
b.access(BAT_READ);

print([count](rangesplit(b,0)));
print([count](rangesplit(b,-1)));
print([count](rangesplit(b,1)));
print([count](rangesplit(b,2)));
print([count](rangesplit(b,5)));
print([count](rangesplit(b,n)));
print([count](rangesplit(b,n + 1)));

print([count](hashsplit(b,0)));
print([count](hashsplit(b,-1)));
print([count](hashsplit(b,2)));
print([count](hashsplit(b,5)));
print([count](hashsplit(b,1)));
print([count](hashsplit(b,n)));
print([count](hashsplit(b,n + 1)));
