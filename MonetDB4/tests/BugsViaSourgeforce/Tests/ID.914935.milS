
VAR b := new(int,int);
VAR n := 10;
VAR i := 0;

WHILE (i < n) {
	i :+= 1;
	b.insert(i,i);
}
b.access(BAT_READ);

print(rangesplit(b,0));
print(rangesplit(b,-1));
print(rangesplit(b,1));
print(rangesplit(b,2));
print(rangesplit(b,5));
print(rangesplit(b,n));
print(rangesplit(b,n + 1));

print(hashsplit(b,0));
print(hashsplit(b,-1));
print(hashsplit(b,2));
print(hashsplit(b,5));
print(hashsplit(b,1));
print(hashsplit(b,n));
print(hashsplit(b,n + 1));
