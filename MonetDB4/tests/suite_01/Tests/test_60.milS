proc p(any B, any C) : any { 
        var local :=newid(B,C); 
	print(local);
}

proc s(int low, int high) : void {
	var t := select(bat("tenk1_unique1"),low,high); 
        t.count().print();
}

proc echo(any x,any y) : any { 
	var z:=x; 
	return(z); 
}
proc New(any N,any A,any Ahsh) : void {
        var Nh := newid(N,"_hsh"); 
        var A := new(int,int,1); 
        rename(A,N); 
        var Ahsh := new(oid,int,1); 
        rename(Ahsh,Nh); 
	print(bat(Nh));
}

