# >Description:
# A ghost BUN appears in the following script:
# 
# script
# 8<-----
# VAR left := new(int,int);
# left.insert(4,-3);
# left.insert(3,-2);
# left.insert(2,-1);
# left.insert(1,0);
#  
# VAR right := new(int,str);
# right.insert(-3,"vier");
# right.insert(-2,"drie");
# right.insert(-1,"twee");
# right.insert(0,"een");
# 
# join(left,right).print;
# 
# 8<-----
# output
# 8<-----
# >join(left,right).print;
# #-----------------#
# # BAT:    tmp_37  #
# # (int)   (str)   #
# #-----------------#
# [ 4,      "vier"  ]
# [ 3,      "drie"  ]
# [ 2,      "twee"  ]
# [ 1,      "een"   ]
# [ 1,      ""      ]

setoid(oid(20000000));

VAR left := new(int,int);
left.insert(4,-3);
left.insert(3,-2);
left.insert(2,-1);
left.insert(1,0);
 
VAR right := new(int,str);
right.insert(-3,"vier");
right.insert(-2,"drie");
right.insert(-1,"twee");
right.insert(0,"een");

join(left,right).print();

quit();
