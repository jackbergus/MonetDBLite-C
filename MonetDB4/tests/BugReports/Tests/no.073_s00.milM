var ab := new(int,str);
ab.rename("ab");
ab.persists(true);
ab.commit();
var ba := ab.reverse();
ba.rename("ba");
ba.persists(true);
ba.commit();
ab.insert(1,"een");
ba.insert("twee",2);
ab.print();
ba.print();
commit();
quit();
