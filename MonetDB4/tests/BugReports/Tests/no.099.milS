setoid(oid(20000000));
PROC comp(any left, any right) : any {
  IF (left = right) {
    printf("left equals right\n");
  } ELSE {
    printf("left is not equal to right\n");
  }
  IF (left != right) {
    printf("left is not equal to right\n");
  } ELSE {
    printf("left equals right\n");
  }
}
comp(oid(nil),oid(nil));
comp(oid(1),oid(nil));
comp(oid(1),oid(1));
comp(str(nil),str(nil));
comp(str("a"),str(nil));
comp(str("a"),str("a"));
quit();
