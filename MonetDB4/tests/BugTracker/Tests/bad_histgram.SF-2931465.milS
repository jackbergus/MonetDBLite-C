module(unix);
module(ascii_io);
var TSTSRCDIR := getenv("TSTSRCDIR");
var td := new(oid,str).import(TSTSRCDIR+"/bad_histgram.SF-2931465.csv");
printf("\nHISTOGRAM REPORTS COUNT 2 FOR STRING \"tragedy\", BUT IT IS UNIQUE\n");
printf("td.select(\"tragedy\").count(): %d\n", td.select("tragedy").count());
printf("td.histogram().find(\"tragedy\"): %d\n", td.histogram().find("tragedy"));

printf("\nTHE SAME IS TRUE FOR ALL STRINGS\n");
printf("td.count(): %d\n", td.count());
printf("td.histogram().count(): %d\n", td.histogram().count());
printf("td.histogram().select(2).count(): %d\n", td.histogram().select(2).count());
