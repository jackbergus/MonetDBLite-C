PROC check_cap(BAT _b) : void {
	VAR inserted := count(_b);
	VAR deleted := count(delta(_b));
	VAR cap := capacity(_b);
	VAR overflow := (inserted + deleted) - cap;
	IF (overflow > lng(0)) {
		printf("!ERROR: wrote %d BUNs beyond capacity\n",overflow);
	}
}

