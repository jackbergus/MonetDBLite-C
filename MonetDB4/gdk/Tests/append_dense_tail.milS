# create an empty [void,oid] BAT with dense tail
var a := new(void,oid).seqbase(0@0).append(1@0).delete(0@0);
# create a non-empty [void,oid] BAT with non-dense tail
var b := new(void,oid).seqbase(0@0).append(0@0).append(0@0).append(0@0);
# append the latter to the former
var c := a.append(b);
c.print();
# check important properties
new(void,str).append("hseqbase").append("hdense").append("tseqbase").append("tdense").reverse().mirror().join(c.info()).print();
