# testing batcalc isnull versions

bi:= bat.new(:int,:int);
bat.insert(bi,1,2);
bat.insert(bi,1,nil:int);
zi:= batcalc.isnil(bi);
io.print(zi);

bb:= bat.new(:int,:bit);
bat.insert(bb,2,true);
bat.insert(bb,2,nil:bit);
bat.insert(bb,2,false);
zb:= batcalc.isnil(bb);
io.print(zb);

bf:= bat.new(:int,:flt);
bat.insert(bf,3,2.0);
bat.insert(bf,3,nil:flt);
fi:= batcalc.isnil(bf);
io.print(fi);

bs:= bat.new(:int,:str);
bat.insert(bs,4,"hello");
bat.insert(bs,4,nil:str);
bat.insert(bs,4,"");
zs:= batcalc.isnil(bs);
io.print(zs);

bo:= bat.new(:int,:oid);
bat.insert(bo,5,0@0);
bat.insert(bo,5,nil:oid);
fo:= batcalc.isnil(bo);
io.print(fo);

bc:= bat.new(:int,:chr);
bat.insert(bc,6,'a');
bat.insert(bc,6,nil:chr);
fc:= batcalc.isnil(bc);
io.print(fc);
