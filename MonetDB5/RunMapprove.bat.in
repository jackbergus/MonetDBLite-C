@echo off

if not "%1"=="" goto skip
call %0 -r
goto end

:skip

setlocal

set pkg=monetdb5
set buildbase=@XBUILD@
set builddir=%buildbase%\MonetDB5
set srcdir=@XSOURCE@\MonetDB5

set PATH=%builddir%\conf;%PATH%

REM execute Mapprove.py in the source directory
pushd %srcdir%

call Mapprove.py --config="%builddir%/conf/monetdb5.conf" --package=%pkg% "--TSTTRGBASE=%builddir%" %*

popd
endlocal

:end
