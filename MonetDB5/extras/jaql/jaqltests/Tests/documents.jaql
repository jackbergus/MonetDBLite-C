# shouldn't exist
load("doctest");

# create one
[1,2.0,"drie"] -> store("doctest");

# re-retrieve it
load("doctest");

# drop it again
drop("doctest");

# hence, shouldn't exist any more
load("doctest");

# and we can store another thing under that name
[4,5,6] -> store("doctest");

# verify it's another document
load("doctest");

# cleanup
drop("doctest");
