s := "hello";
i := 1;
f := 3.14;
c := 'a';
b:= true;
bb:= bat.new(:void,:str);

st:= inspect.getType(s); io.printf("string %d\n",st);
it:= inspect.getType(i); io.printf("int %d\n",it);
ft:= inspect.getType(f); io.printf("flt %d\n",ft);
ct:= inspect.getType(c); io.printf("chr %d\n",ct);
bt:= inspect.getType(b); io.printf("bit %d\n",bt);
bbt:= inspect.getType(bb); io.printf("bat %d\n",bbt);

# get them as strings
v:= "";
v:= inspect.getTypeName(st); io.printf("string %s\n",v);
v:= inspect.getTypeName(it); io.printf("int %s\n",v);
v:= inspect.getTypeName(ft); io.printf("flt %s\n",v);
v:= inspect.getTypeName(ct); io.printf("chr %s\n",v);
v:= inspect.getTypeName(bt); io.printf("bit %s\n",v);
v:= inspect.getTypeName(bbt); io.printf("bat %s\n",v);

