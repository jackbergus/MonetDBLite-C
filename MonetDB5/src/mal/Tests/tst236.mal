# check reference counting and multiplex
function dir():void;
        d:= bbp.getRefCount();
        dl:=bbp.getLRefCount();
        dn:= bbp.getNames();
        io.print(dn,d,dl);
end dir;

function createBAT(name:str):bat;
	res:= bat.new(:int,:int);
	bat.setName(res,name);
	i:= calc.getBATidentifier(res);
	return i;
end createBAT;
