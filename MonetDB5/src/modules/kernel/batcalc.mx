@= accumUnary
	{
		@1* p = (@1*)Tloc(b,BUNfirst(b));
		@1 *q = (@1*)Tloc(b,BUNlast(b));
		@1 *o = (@1*)Tloc(bn,BUNfirst(bn));

		if (b->T->nonil)
			for(; p<q; p++, o++)
				*o = @2 *p;
		else
