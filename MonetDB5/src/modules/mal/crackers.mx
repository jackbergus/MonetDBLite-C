@= JoinSelectFunctions
mal_export str CRKjoinSelectBounds_@1(int *res, int *right, @1 *low, @1 *hgh, bit *inclusiveLow, bit *inclusiveHgh, int *left);
str
CRKjoinSelectBounds_@1(int *res, int *right, @1 *low, @1 *hgh, bit *inclusiveLow, bit *inclusiveHgh, int *left)
{
        if (@2_EQ(low,ATOMnilptr(TYPE_@1),@3@1)){
		if (*inclusiveHgh==TRUE)
                	JoinSelectSingle_RE_@1(res, left, right, hgh);
		else 
                	JoinSelectSingle_RO_@1(res, left, right, hgh);
	}
        else if (@2_EQ(hgh,ATOMnilptr(TYPE_@1),@3@1))
		if (*inclusiveLow==TRUE)
                	JoinSelectSingle_LE_@1(res, left, right, low);
		else 
                	JoinSelectSingle_LO_@1(res, left, right, low);
        else{
		if ((*inclusiveLow==TRUE) && (*inclusiveHgh==TRUE))
                	JoinSelectRange_LE_RE_@1(res, left, right, low, hgh);
		else 
		if ((*inclusiveLow==TRUE) && (*inclusiveHgh==FALSE))
                	JoinSelectRange_LE_RO_@1(res, left, right, low, hgh);
		else 
		if ((*inclusiveLow==FALSE) && (*inclusiveHgh==FALSE))
                	JoinSelectRange_LO_RO_@1(res, left, right, low, hgh);
		else 
		if ((*inclusiveLow==TRUE) && (*inclusiveHgh==TRUE))
                	JoinSelectRange_LE_RE_@1(res, left, right, low, hgh);
	}
        return MAL_SUCCEED;
}

