		BATsave(b);
		/*
		resMmap = BATmmap(b,mode,mode,mode);             
	        printf("\n result--> %d",resMmap);
                printf("\ncopyM %d\n",b->batMapbuns);
                printf("\noriginalM %d\n",bo->batMapbuns);
