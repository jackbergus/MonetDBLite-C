#include "mal.h"
#include "mal_exception.h"
#include "crackers.h"
#include "mtime.h"
#include "bat.h"

#define MAXPIECE 1024*100

static int maxCrackMap = 0;

static CrackIndex *Index;
static CrackTreeIndex *TreeIndex;

int IndexSize = 0;
int IndexStop = 250000;

int
Height(struct Node * x){
        if( x == NULL )
                return -1;
        else
                return x->height;
}

struct Node *
SingleRotateLeft(int m, struct Node * node){
        struct Node * l;

        l = node->left;
        node->left = l->right;
        l->right = node;

	if (node->head == TRUE){
		node->head = FALSE;
		l->head = TRUE;
		TreeIndex[m].Tree = l;
	}

	int nodeLh = Height(node->left);
	int nodeRh = Height(node->right);
	int lh = Height(l->left);
     
	node->height = (nodeLh > nodeRh ? nodeLh : nodeRh) + 1;
        l->height = (lh > node->height ? lh : node->height) + 1;

        return l;
}

struct Node *
SingleRotateRight(int m, struct Node * node){
        struct Node * r;

        r = node->right;
        node->right = r->left;
        r->left = node;

	if (node->head == TRUE){
		node->head = FALSE;
		r->head = TRUE;
		TreeIndex[m].Tree = r;
	}

	int nodeLh = Height(node->left);
	int nodeRh = Height(node->right);
	int rh = Height(r->right);
     
	node->height = (nodeLh > nodeRh ? nodeLh : nodeRh) + 1;
        r->height = (rh > node->height ? rh : node->height) + 1;

        return r;
}

struct Node *
DoubleRotateLeft(int m, struct Node * node){
            node->left = SingleRotateRight(m, node->left);
            return SingleRotateLeft(m, node);
}

struct Node *
DoubleRotateRight(int m, struct Node * node){
            node->right = SingleRotateLeft(m, node->right);
            return SingleRotateRight(m, node);
}



@= TreeOperations

lng
getNext_@1(struct Node * current, BAT *b, BUN base, int xx, lng next){

	if (current->right == NULL)
		return next;

	struct Node * temp = current->right;
	while(temp->left != NULL)
		temp = temp->left;
	
	return *(lng*)BUNhloc(b,base + ((temp->position) * xx));
}

bit
GetLow_@1(@1  x, bit inclusive, struct Node * current, BAT *b, BUN base, int xx, lng *p1, lng *p2, lng previous, lng next){

	BUN cur = base + ((current->position) * xx);
	BUN curValue = BUNtloc(b, cur);

	/* For the case where the head is the needed value. Maybe could be done outside to avoid one function call*/
	if ( @2_EQ(&x,curValue,@3@1) && inclusive == current->inclusive){
		*p1 = *(lng*)BUNhloc(b, cur);	
		return 1;
	}	

	if (current->left == NULL){

		/*crack from the begining of the BAT if this was the last index entry but the needed value is still smaller*/
       	 	if ( @2_LT(&x,curValue,@3@1) || (@2_EQ(&x,curValue,@3@1) && inclusive == TRUE && current->inclusive == FALSE) ){
		
                	*p1 = previous;
			*p2 = *(lng*)BUNhloc(b, cur);
			return 0;
		}
		
		goto checkRight;
	
	}

	BUN Lchild = base + ((current->left->position) * xx);
	BUN LchildValue = BUNtloc(b, Lchild);

	/*An exact match*/
	if (@2_EQ(&x,LchildValue,@3@1) && inclusive == current->left->inclusive){ 
		*p1 = *(lng*)BUNhloc(b, Lchild);
		/* *p2 = getNext_@1(current->left, b, base, xx, *(lng*)BUNhloc(b, cur));*/
		return 1;
	}

	/*if it is smaller than the current check for the left one*/
        if( @2_LT(&x,curValue,@3@1) || (@2_EQ(&x,curValue,@3@1) && inclusive == TRUE && current->inclusive == FALSE) )
      		return GetLow_@1(x, inclusive, current->left, b, base, xx, p1, p2, previous, *(lng*)BUNhloc(b, cur));

	checkRight:
	if (current->right == NULL){
		
                *p1 = *(lng*)BUNhloc(b, cur);
		*p2 = next;
		return 0;
	}	

	BUN Rchild = base + ((current->right->position) * xx);
	BUN RchildValue = BUNtloc(b, Rchild);
       
	/*An exact match*/
	if (@2_EQ(&x,RchildValue,@3@1) && inclusive == current->right->inclusive){ 
		*p1 = *(lng*)BUNhloc(b, Rchild);
		/* *p2 =  getNext_@1(current->right, b, base, xx, next);*/
		return 1;
	}

	return GetLow_@1(x, inclusive, current->right, b, base, xx, p1, p2, *(lng*)BUNhloc(b, cur), next);
 
}


lng
getPrevious_@1(struct Node * current, BAT *b, BUN base, int xx, lng previous){

	if (current->left == NULL)
		return previous;

	struct Node * temp = current->left;
	while(temp->right != NULL)
		temp = temp->right;
	
	return *(lng*)BUNhloc(b,base + ((temp->position) * xx));
}

bit
GetHgh_@1(@1  x, bit inclusive, struct Node * current, BAT *b, BUN base, int xx, lng *p1, lng *p2, lng previous, lng next){
	
	BUN cur = base + ((current->position) * xx);
	BUN curValue = BUNtloc(b, cur);

	/* For the case where the head is the needed value. Maybe could be done outside to avoid one function call*/
	if ( @2_EQ(&x,curValue,@3@1) && inclusive == current->inclusive){
		*p2 = *(lng*)BUNhloc(b, cur);	
		return 1;
	}	


	if (current->right == NULL){

		/*crack until the end of the BAT if this was the last index entry but the needed value is still bigger*/
       	 	if ( @2_GT(&x,curValue,@3@1) || (@2_EQ(&x,curValue,@3@1) && inclusive == FALSE && current->inclusive == TRUE) ){
			*p1 = *(lng*)BUNhloc(b, cur);
                	*p2 = next; 
			return 0;
		}
	
		goto checkLeft;

	}

	BUN Rchild = base + ((current->right->position) * xx);
	BUN RchildValue = BUNtloc(b, Rchild);

	/*An exact match*/
	if (@2_EQ(&x,RchildValue,@3@1) && inclusive == current->right->inclusive){ 
		/* *p1 = getPrevious_@1(current->right, b, base, xx, *(lng*)BUNhloc(b, cur));*/
		*p2 = *(lng*)BUNhloc(b, Rchild);
		return 1;
	}

	/*if it is bigger than the current check for the right one*/
        if( @2_GT(&x,curValue,@3@1) || (@2_EQ(&x,curValue,@3@1) && inclusive == FALSE && current->inclusive == TRUE) )
      		return GetHgh_@1(x, inclusive, current->right, b, base, xx, p1, p2, *(lng*)BUNhloc(b, cur), next);

	checkLeft:
	if (current->left == NULL){
		*p1 = previous;
               	*p2 = *(lng*)BUNhloc(b, cur);
		return 0;
	}
		
	BUN Lchild = base + ((current->left->position) * xx);
        BUN LchildValue = BUNtloc(b, Lchild);

	/*An exact match*/
	if (@2_EQ(&x,LchildValue,@3@1) && inclusive == current->left->inclusive){ 
		/* *p1 = getPrevious_@1(current->left, b, base, xx, previous);*/
		*p2 = *(lng*)BUNhloc(b, Lchild);
		return 1;
	}

	return GetHgh_@1(x, inclusive, current->left, b, base, xx, p1, p2, previous, *(lng*)BUNhloc(b, cur));	
 
}


struct Node *
InsertTree_@1(int m, int position, @1 value, bit inclusive, struct Node * current, BAT * b, BUN base, int xx){
	
	if (current == NULL){
        	current = (struct Node *)GDKmalloc(sizeof(struct Node));
                current->position = position; 
                current->inclusive = inclusive; 
		current->height = 0;
                current->left = NULL;
		current->right = NULL;
		current->head = FALSE;
		goto end;
        }
        
	BUN cur = base + ((current->position) * xx);
        BUN curValue = BUNtloc(b, cur);
	
        if ( @2_LT(&value,curValue,@3@1) || (@2_EQ(&value,curValue,@3@1) && inclusive == TRUE && current->inclusive == FALSE) ){

                current->left = InsertTree_@1(m, position, value, inclusive, current->left, b, base, xx);

                if ( Height(current->left) - Height(current->right) == 2 ){
		        BUN Lchild = base + ((current->left->position) * xx);
        		BUN LchildValue = BUNtloc(b, Lchild);

                    	if ( @2_LT(&value,LchildValue,@3@1) || (@2_EQ(&value,LchildValue,@3@1) && inclusive == TRUE && current->left->inclusive == FALSE) )
                        	current = SingleRotateLeft(m, current);
                    	else
                        	current = DoubleRotateLeft(m, current);
		}
        }
        else
        if ( @2_GT(&value,curValue,@3@1) || (@2_EQ(&value,curValue,@3@1) && inclusive == FALSE && current->inclusive == TRUE) ){

                current->right = InsertTree_@1(m, position, value, inclusive, current->right, b, base, xx);

                if ( Height(current->right) - Height(current->left) == 2 ){
			BUN Rchild = base + ((current->right->position) * xx);
                        BUN RchildValue = BUNtloc(b, Rchild);

                        if ( @2_GT(&value,RchildValue,@3@1) || (@2_EQ(&value,RchildValue,@3@1) && inclusive == FALSE && current->right->inclusive == TRUE) )
                        	current = SingleRotateRight(m, current);
                    	else
                        	current = DoubleRotateRight(m, current);
		}
        }

	end:;
	int lh = Height(current->left);
	int rh = Height(current->right);
		
        current->height = (lh > rh? lh : rh) + 1;
        return current;
}

