	map->pieces[0].rng_min = INT_MIN;
	map->pieces[0].rng_max = INT_MAX;
	map->pieces[0].inclusiveMin = TRUE;
	map->pieces[0].inclusiveMax = TRUE;
	map->pieces[0].real_rng_min = INT_MIN;
