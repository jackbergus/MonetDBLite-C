str
removeTreeIndex(int *k, int *bid){
	(void) k;
	BAT *c;
	int position = -1;
	for (int i = 0; i < maxCrackMap; i++)
        	if (TreeIndex[i].bid == *bid){
                	position = i;	
			break;
		}
	/*if (position == -1) return; Trying to erase an index that does not exist*/

	/*Delete index BAT*/
        if ((c = BATdescriptor(TreeIndex[position].cid)) == NULL) {
                fprintf(stderr,"crackers.AddIndex: Cannot access crack index");
		return "FAILED";
        }

	BATdelete(c);
	TreeIndex[position].cid = -1;
	BBPunfix(c->batCacheid);

	/*Delete Tree*/
	FreeAVLTree(TreeIndex[position].Tree);

	TreeIndex[position].Tree = NULL;
	
	return MAL_SUCCEED;
}

str
appendCrackerBAT(int *k, int * bid, int * new){
	(void)k;
	int position = -1;
	for (int i = 0; i < maxCrackMap; i++)
                if (TreeIndex[i].bid == *bid){
                        position = i;
			break;	
		}

        BAT *i, *u;

        if ((i = BATdescriptor(TreeIndex[position].cbid)) == NULL) {
                return throwMessage("bat.append", "Cannot access descriptor");
        }
        if ((u = BATdescriptor(*new)) == NULL) {
                BBPreleaseref(i->batCacheid);
                return throwMessage("bat.append", "Cannot access descriptor");
        }
        BATappend(i, u, TRUE);
       
	printf("\n index size of bat is "SZFMT" \n", BATcount(i));
	printf("\n update size of bat is "SZFMT" \n", BATcount(u));
	BBPunfix(i->batCacheid);
	BBPunfix(u->batCacheid);

	removeTreeIndex(k,bid);

	return MAL_SUCCEED;
}

void
printCrackIndex(int bid)
{
	int i = existsIndex(bid);
	if (i == -1)
		fprintf(stderr, " the crack index does not exist \n");
	BATprint(BATdescriptor(Index[i].cid));
}

str
CRKprintIndex(int *k, int *bid)
{
	(void)k;
	printCrackIndex(*bid);
	return MAL_SUCCEED;
}


