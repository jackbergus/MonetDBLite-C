function gen(n:int):bat[:oid,:int];
	r:=bat.new(:oid,:int);
	barrier (go,i):=language.newRange(0:int);
                bat.append(r,i);
                redo (go,i):= language.nextElement(1:int,n);
        exit (go,i);
	return r;
end gen;

