bpm.open();
bpm.dump();

function user.partinfo(Ra:bat[:any_1,:any_2]):void;
barrier Rp:= bpm.newIterator(Ra);
	Ri:= bat.info(Rp);
	io.print(Ri);
	redo Rp:= bpm.hasMoreElements(Ra);
exit Rp;
end partinfo;

f:bat[:oid,:flt]:=bpm.take("myflt");
user.partinfo(f);
bpm.discard(f);

r:bat[:oid,:sht]:=bpm.take("myR");
user.partinfo(r);
bpm.discard(r);

bpm.saveCatalog();
bpm.dump();
