			/*
			else if (newPosition < cprevPos@3) 
				low = mid;
			else 
				hgh = mid;
			oldmid = mid;		
			mid    = (hgh - low)/2 +low;
			sample = FALSE;
			if (oldmid == mid || mid == *min@3 || mid == *max@3){
				activeCrack = FALSE;
				break;
			}
			*/
		}
		if (activeCrack){	
			curNode@3 = addCrackerIndex_@1(mapPos@3, &mid, TRUE, newPosition, index@3);
			AddToTape_@1(mapPos@3, @1_nil, mid, TRUE, TRUE);
