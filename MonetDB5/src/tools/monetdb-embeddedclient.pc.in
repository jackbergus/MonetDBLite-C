Requires: monetdb-gdk = @MONETDB_REQUIRED_VERSION@ monetdb-mutils = @MONETDB_REQUIRED_VERSION@ monetdb-stream = @MONETDB_REQUIRED_VERSION@ monetdb-mapi = @CLIENTS_REQUIRED_VERSION@

Libs: -L${libdir} -lembeddedmal
Cflags: -I${includedir}/MonetDB5/tools
