@echo off

if not "%1"=="" goto skip
call %0 -r
goto end

:skip

setlocal

rem default back end is MonetDB5
set V=5
if "%1" == "-4" set V=4& shift
if "%1" == "-5" set V=5& shift

set pkg=monetdb-sql
set buildbase=@XBUILD@
set builddir=%buildbase%\geom
set srcdir=@XSOURCE@\geom

set PATH=%builddir%\conf;%PATH%

REM execute Mapprove.py in the source directory
pushd %srcdir%

call "%buildbase%\testing\src\Mapprove.py" -%V% "--package=%pkg%" "--TSTSRCBASE=%srcdir%" "--TSTTRGBASE=%builddir%" %1 %2 %3 %4 %5 %6 %7 %8 %9

popd
endlocal

:end
