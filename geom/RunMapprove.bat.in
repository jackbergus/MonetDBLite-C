@echo off

if not "%1"=="" goto skip
call %0 -r
goto end

:skip

setlocal

set pkg=monetdb-sql
set builddir=@XGEOM_BUILD@
set srcdir=@XGEOM_SOURCE@

set PATH=%builddir%\conf;%PATH%

REM execute Mapprove.py in the source directory
pushd %srcdir%

call Mapprove.py "--package=%pkg%" "--TSTTRGBASE=%builddir%" %1 %2 %3 %4 %5 %6 %7 %8 %9

popd
endlocal

:end
