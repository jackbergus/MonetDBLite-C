# marking a void-T table nil
#
# marking a void-T table nil gives incorrect results. 
#
# (detected in MonetDB_TRECvid branch)

# BUG: void behaves differently from oid under mark

var t  := new(oid,str);
t.insert( oid(0), "a" );
t.insert( oid(1), "b" );
t.insert( oid(2), "c" );
t.insert( oid(3), "d" );
t.insert( oid(4), "e" );
t.insert( oid(5), "f" );
t.insert( oid(6), "g" );
t.insert( oid(7), "h" );
t.project(nil).print();

t  := new(void,str).seqbase(oid(0));
t.insert( oid(0), "a" );
t.insert( oid(1), "b" );
t.insert( oid(2), "c" );
t.insert( oid(3), "d" );
t.insert( oid(4), "e" );
t.insert( oid(5), "f" );
t.insert( oid(6), "g" );
t.insert( oid(7), "h" );
t.project(nil).print();
