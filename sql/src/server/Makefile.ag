## Process this file with automake to produce Makefile.in

INCLUDES = ../include ../common ../storage $(MONET5_INCS) $(MONETDB_INCS)

YFLAGS = -d -p sql -v

#  (At least on Fedora Core 4,) bison 2.0 seems to generate code
#  that icc does not like; since the problem only occurs with
#  sql/src/server/sql_parser.mx & amdb/src/lang/parser.y,
#  we "mis-use" the NO_INLINE_CFLAGS
#  to disable the respective warning as locally as possible
#  via "-wd592" (#592: variable "." is used before its value is set)
#  (see also MonetDB/conf/monet.m4 & amdb/src/lang/Makefile.ag).
CFLAGS = $(NO_INLINE_CFLAGS)

lib_sqlserver = {
	NOINST
	DIR = libdir
	SOURCES = \
		sql_atom.mx \
		sql_datetime.mx \
		sql_decimal.mx \
		sql_scope.mx \
		sql_statement.mx \
		sql_trans.mx \
		sql_schema.mx \
		sql_updates.mx \
		sql_select.mx \
		sql_semantic.mx \
		sql_optimize.mx \ 
		sql_rel2bin.mx \
		sql_qc.mx \
		sql_symbol.mx \
		sql_scan.mx \   
		sql_parser.mx \  
		sql_mvc.mx \    
		sql_env.mx \    
		sql_privileges.mx \  
		sql_sequence.mx \  
		sql_psm.mx \  
		rel_semantic.mx \
		rel_select.mx \
		rel_bin.mx \
		rel_dump.mx
}

