@echo off

if not "%1"=="" goto skip
call %0 -r
goto end

:skip

setlocal

set pkg=monetdb-sql
set buildbase=@XBUILD@
set builddir=%buildbase%\sql
set srcdir=@XSOURCE@\sql

set PYTHONPATH=%buildbase%\testing\src;%srcdir%\..\testing\src;%PYTHONPATH%

REM execute Mapprove.py in the source directory
pushd %srcdir%

call "%buildbase%\testing\src\Mapprove.py" "--package=%pkg%" "--TSTSRCBASE=%srcdir%" "--TSTTRGBASE=%builddir%" %1 %2 %3 %4 %5 %6 %7 %8 %9

popd
endlocal

:end
