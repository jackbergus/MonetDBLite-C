@echo off

if not "%1"=="" goto skip
call %0 -r
goto end

:skip

setlocal

set pkg=monetdb-clients
set builddir=@XCLIENTS_BUILD@
set srcdir=@XCLIENTS_SOURCE@

set PATH=%builddir%\conf;%PATH%

REM execute Mapprove.py in the source directory
pushd %srcdir%

call Mapprove.py --package=%pkg% "--TSTTRGBASE=%builddir%" %*

popd
endlocal

:end
