@f epilogue
@a Niels Nes

@h
#ifndef _EPILOGUE_H_
#define _EPILOGUE_H_

#include "symbol.h"

class Epilogue:public Symbol {
      public:
	Epilogue(int t, char *n);

	virtual ostream & print(language * l, ostream & o) const;
};
