@C
#include <mel_config.h>
#include "epilogue.h"
#include "language.h"
#include <string.h>


Epilogue::Epilogue(int t, char *n) : Symbol(t,n)
{
}

ostream &
Epilogue::print(language *l, ostream &o) const
{
	return l->gen_epilogue(o, *this);
}
