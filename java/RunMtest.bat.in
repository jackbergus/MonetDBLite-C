@echo off

if not "%1"=="" goto skip
call %0 -rq
goto end

:skip

setlocal

set pkg=java
set buildbase=@XBUILD@
set builddir=%buildbase%\java
set srcdir=@XSOURCE@\java

set PATH=%buildbase%\testing\src;%PATH%

set PYTHONPATH=%buildbase%\testing\src;%srcdir%\..\testing\src;%PYTHONPATH%

REM execute Mtest.py in the source directory
pushd %srcdir%

call "%buildbase%\testing\src\Mtest.py" --package=%pkg% "--dbfarm=%builddir%\dbfarm" "--TSTTRGBASE=%builddir%" %*

popd
endlocal

:end
